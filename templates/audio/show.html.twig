{% extends 'base.html.twig' %}

{% block title %}Audio{% endblock %}

{% block body %}
    <h1>Audio</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Name</th>
                <td>{{ audio.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ audio.description }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ audio.url }}</td>
            </tr>
            <tr>
                <th>Size</th>
                <td>{{ audio.size }}</td>
            </tr>
            <tr>
                <th>Format</th>
                <td>{{ audio.format }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ audio.id }}</td>
            </tr>
            <tr>
                <th>Duration</th>
                <td>{{ audio.duration }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_audio_index') }}">back to list</a>

    <a href="{{ path('app_audio_edit', {'id': audio.id}) }}">edit</a>

    {{ include('audio/_delete_form.html.twig') }}
{% endblock %}
